{
  "name": "virtue-ui",
  "version": "1.0.2",
  "repository": {
    "url": "https://github.com/virtueorg/ui"
  },
  "author": {
    "name": "Leonardo Dipace",
    "email": "leonardodipace@gmail.com",
    "url": "https://github.com/leonardodipace"
  },
  "contributors": [
    {
      "name": "Stefano Labianca",
      "email": "lstefano@proton.me",
      "url": "https://github.com/Stefano-Labianca"
    }
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.0",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "8.56.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.2.7",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.3",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@melt-ui/svelte": "^0.68.0",
    "clsx": "^2.1.0",
    "nanoid": "^5.0.5",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.1.20"
  }
}
